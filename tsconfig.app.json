/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@styles/*": ["src/styles/*"],
      "@model/*": ["src/app/core/model/*"],
      "@service/*": ["src/app/core/service/*"],
      "@constant/*": ["src/app/core/constant/*"],
    },
    "outDir": "./out-tsc/app",
    "types": [
      "node"
    ]
  },
  "files": [
    "src/main.ts",
    "src/main.server.ts",
    "server.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
